{
  "rules": {
      "semi": ["error", "always"],
      "quotes": ["warning", "single"],
      "comma-dangle": ["error", {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
    }]
  },
  "parser": "babel-eslint"
}